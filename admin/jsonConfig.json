{
  "i18n": true,
  "type": "tabs",
  "iconPosition": "top",
  "items": {
    "_general": {
      "type": "panel",
      "label": "General",
      "icon": "data:image/svg+xml;base64,",
      "items": {
        "bind": {
          "newLine": true,
          "type": "ip",
          "label": "IP",
          "listenOnAllPorts": true,
          "sm": 12,
          "md": 4,
          "lg": 4
        },
        "port": {
          "type": "number",
          "min": 1,
          "max": 65535,
          "sm": 12,
          "md": 4,
          "lg": 4,
          "label": "Port"
        },
        "secure": {
          "newLine": true,
          "type": "checkbox",
          "sm": 12,
          "md": 4,
          "lg": 4,
          "label": "Secure (HTTPS)"
        },
        "certPublic": {
          "hidden": "!data.secure",
          "type": "certificate",
          "certType": "public",
          "sm": 12,
          "md": 4,
          "lg": 4,
          "label": "Public certificate"
        },
        "certPrivate": {
          "hidden": "!data.secure",
          "type": "certificate",
          "certType": "private",
          "sm": 12,
          "md": 4,
          "lg": 4,
          "label": "Private certificate"
        },
        "httpAdminRoot": {
          "newLine": true,
          "type": "text",
          "sm": 12,
          "md": 4,
          "lg": 4,
          "label": "http Admin directory",
          "help": "Root directory for Node-RED (e.g. /admin)"
        },
        "httpNodeRoot": {
          "type": "text",
          "sm": 12,
          "md": 4,
          "lg": 4,
          "label": "http Node directory",
          "help": "Root directory for HTTP Nodes and Dashboard (e.g. /)"
        },
        "httpStatic": {
          "type": "text",
          "sm": 12,
          "md": 4,
          "lg": 4,
          "label": "http Static directory",
          "help": "Can be used to identify a directory of static content on local host"
        },
        "maxMemory": {
          "newLine": true,
          "type": "number",
          "min": 32,
          "sm": 12,
          "md": 4,
          "lg": 4,
          "label": "Max. allocated RAM"
        },
        "npmLibs": {
          "type": "chips",
          "sm": 12,
          "md": 8,
          "lg": 8,
          "label": "Additional npm packages"
        },
        "_headerAdditionalFeatures": {
          "newLine": true,
          "type": "header",
          "size": 3,
          "sm": 12,
          "md": 12,
          "lg": 12,
          "text": "Additional features"
        },
        "valueConvert": {
          "newLine": true,
          "type": "checkbox",
          "sm": 12,
          "md": 4,
          "lg": 4,
          "label": "Convert state values to strings"
        },
        "palletmanagerEnabled": {
          "newLine": true,
          "type": "checkbox",
          "sm": 12,
          "md": 4,
          "lg": 4,
          "label": "Use palletmanager"
        },
        "_backupWarningPallet": {
          "hidden": "!data.palletmanagerEnabled",
          "type": "staticText",
          "text": "Backups will not contain any modules installed via Palletmanager!",
          "style": {
            "backgroundColor": "#a83232",
            "borderRadius": "10px",
            "fontSize": "20px",
            "color": "#ffffff",
            "textAlign": "center",
            "marginTop": "5px"
          },
          "sm": 12,
          "md": 8,
          "lg": 8
        },
        "projectsEnabled": {
          "newLine": true,
          "type": "checkbox",
          "sm": 12,
          "md": 4,
          "lg": 4,
          "label": "Enable Projects feature"
        },
        "_backupWarningProjects": {
          "hidden": "!data.projectsEnabled",
          "type": "staticText",
          "text": "Backups will not contain any projects!",
          "style": {
            "backgroundColor": "#a83232",
            "borderRadius": "10px",
            "fontSize": "20px",
            "color": "#ffffff",
            "textAlign": "center",
            "marginTop": "5px"
          },
          "sm": 12,
          "md": 8,
          "lg": 8
        },
        "allowCreationOfForeignObjects": {
          "newLine": true,
          "type": "checkbox",
          "sm": 12,
          "md": 6,
          "lg": 6,
          "label": "Allow creation of foreign objects"
        },
        "safeMode": {
          "newLine": true,
          "type": "checkbox",
          "sm": 12,
          "md": 6,
          "lg": 6,
          "label": "Safe mode"
        },
        "doNotReadObjectsDynamically": {
          "newLine": true,
          "type": "checkbox",
          "sm": 12,
          "md": 6,
          "lg": 6,
          "label": "Do not read objects from admin in the select object dialog"
        },
        "_update": {
          "newLine": true,
          "type": "sendTo",
          "showProcess": true,
          "command": "update",
          "variant": "outlined",
          "icon": "send",
          "result": {
            "success": {
              "en": "Successfully updated",
              "de": "Erfolgreich aktualisiert",
              "ru": "Успешно обновляется",
              "pt": "Atualizado com sucesso",
              "nl": "Succesvolle update",
              "fr": "Mise à jour réussie",
              "it": "Aggiornato con successo",
              "es": "Actualización exitosa",
              "pl": "Sukcesowo aktualizowany",
              "uk": "Успішно оновлено",
              "zh-cn": "成功更新"
            }
          },
          "label": "Update select dialog"
        }
      }
    },
    "_authentication": {
      "type": "panel",
      "label": "Authentication",
      "icon": "data:image/svg+xml;base64,",
      "items": {
        "authType": {
          "newLine": true,
          "type": "select",
          "options": [
            {
              "label": {
                "en": "None",
                "de": "Keine",
                "ru": "Нет",
                "pt": "Nenhuma",
                "nl": "Niets",
                "fr": "None",
                "it": "Nessuno",
                "es": "Ninguno",
                "pl": "None",
                "uk": "Ні",
                "zh-cn": "无"
              },
              "value": "None"
            },
            {
              "label": {
                "en": "Simple",
                "de": "Einfach",
                "ru": "Просто",
                "pt": "Simples",
                "nl": "Simpel",
                "fr": "Simple",
                "it": "Semplice",
                "es": "Simple",
                "pl": "Prosty",
                "uk": "Прості",
                "zh-cn": "简单"
              },
              "value": "Simple"
            },
            {
              "label": {
                "en": "Extended",
                "de": "Erweiterte",
                "ru": "Расширенный",
                "pt": "Estendido",
                "nl": "Verlengd",
                "fr": "Extended",
                "it": "Esteso",
                "es": "Extended",
                "pl": "Extended",
                "uk": "Продовжити",
                "zh-cn": "参加者"
              },
              "value": "Extended"
            }
          ],
          "sm": 12,
          "md": 4,
          "lg": 4,
          "label": "Authentication type"
        },
        "user": {
          "hidden": "data.authType !== 'Simple'",
          "type": "text",
          "sm": 12,
          "md": 4,
          "lg": 4,
          "label": "User name"
        },
        "pass": {
          "hidden": "data.authType !== 'Simple'",
          "type": "password",
          "sm": 12,
          "md": 4,
          "lg": 4,
          "label": "Password"
        },
        "hasDefaultPermissions": {
          "newLine": true,
          "hidden": "data.authType !== 'Extended'",
          "type": "checkbox",
          "sm": 12,
          "md": 4,
          "lg": 4,
          "label": "Use default permissions"
        },
        "defaultPermissions": {
          "hidden": "data.authType !== 'Extended' || !data.hasDefaultPermissions",
          "type": "text",
          "sm": 12,
          "md": 6,
          "lg": 6,
          "label": "Default permissions"
        },
        "authExt": {
          "newLine": true,
          "hidden": "data.authType !== 'Extended'",
          "type": "table",
          "sm": 12,
          "md": 12,
          "lg": 12,
          "label": "Users",
          "items": [
            {
              "type": "text",
              "attr": "username",
              "width": "25%",
              "title": {
                "en": "User name",
                "de": "Benutzername",
                "ru": "Имя пользователя",
                "pt": "Nome do utilizador",
                "nl": "Noem",
                "fr": "Nom de l ' utilisateur",
                "it": "Nome utente",
                "es": "Nombre de usuario",
                "pl": "Imię User",
                "uk": "Ім'я користувача",
                "zh-cn": "使用名称"
              },
              "filter": true,
              "sort": true,
              "default": "admin"
            },
            {
              "type": "text",
              "attr": "password",
              "width": "25%",
              "title": {
                "en": "Password",
                "de": "Passwort vergessen",
                "ru": "Пароль",
                "pt": "Senha",
                "nl": "Wachtwoord",
                "fr": "Mot de passe",
                "it": "Password",
                "es": "Contraseña",
                "pl": "Password",
                "uk": "Логін",
                "zh-cn": "护照"
              },
              "filter": false,
              "sort": false,
              "default": ""
            },
            {
              "type": "chips",
              "attr": "permissions",
              "width": "50%",
              "title": {
                "en": "Permissions",
                "de": "Berechtigungen",
                "ru": "Разрешения",
                "pt": "Permissões",
                "nl": "Toestemming",
                "fr": "Permissions",
                "it": "Permissioni",
                "es": "Permisos",
                "pl": "Opłata",
                "uk": "Дозвіли",
                "zh-cn": "排放量"
              },
              "filter": false,
              "sort": false
            }
          ]
        }
      }
    }
  }
}
